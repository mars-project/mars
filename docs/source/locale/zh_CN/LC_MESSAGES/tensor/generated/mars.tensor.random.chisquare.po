#: ../../source/tensor/generated/mars.tensor.random.chisquare.rst:2
msgid "mars.tensor.random.chisquare"
msgstr ""

#~ msgid "Draw samples from a chi-square distribution."
#~ msgstr ""

#~ msgid ""
#~ "When `df` independent random variables, "
#~ "each with standard normal distributions "
#~ "(mean 0, variance 1), are squared "
#~ "and summed, the resulting distribution "
#~ "is chi-square (see Notes).  This "
#~ "distribution is often used in hypothesis"
#~ " testing."
#~ msgstr ""

#~ msgid "df"
#~ msgstr ""

#~ msgid "float or array_like of floats"
#~ msgstr ""

#~ msgid "Number of degrees of freedom, should be > 0."
#~ msgstr ""

#~ msgid "size"
#~ msgstr ""

#~ msgid "int or tuple of ints, optional"
#~ msgstr ""

#~ msgid ""
#~ "Output shape.  If the given shape "
#~ "is, e.g., ``(m, n, k)``, then ``m"
#~ " * n * k`` samples are drawn."
#~ "  If size is ``None`` (default), a"
#~ " single value is returned if ``df``"
#~ " is a scalar.  Otherwise, "
#~ "``mt.array(df).size`` samples are drawn."
#~ msgstr ""

#~ msgid "chunk_size"
#~ msgstr ""

#~ msgid "int or tuple of int or tuple of ints, optional"
#~ msgstr ""

#~ msgid "Desired chunk size on each dimension"
#~ msgstr ""

#~ msgid "gpu"
#~ msgstr ""

#~ msgid "bool, optional"
#~ msgstr ""

#~ msgid "Allocate the tensor on GPU if True, False as default"
#~ msgstr ""

#~ msgid "dtype"
#~ msgstr ""

#~ msgid "data-type, optional"
#~ msgstr ""

#~ msgid "Data-type of the returned tensor."
#~ msgstr ""

#~ msgid "out"
#~ msgstr ""

#~ msgid "Tensor or scalar"
#~ msgstr ""

#~ msgid "Drawn samples from the parameterized chi-square distribution."
#~ msgstr ""

#~ msgid "ValueError"
#~ msgstr ""

#~ msgid ""
#~ "When `df` <= 0 or when an "
#~ "inappropriate `size` (e.g. ``size=-1``) is "
#~ "given."
#~ msgstr ""

#~ msgid ""
#~ "The variable obtained by summing the "
#~ "squares of `df` independent, standard "
#~ "normally distributed random variables:"
#~ msgstr ""

#~ msgid "Q = \\sum_{i=0}^{\\mathtt{df}} X^2_i"
#~ msgstr ""

#~ msgid "is chi-square distributed, denoted"
#~ msgstr ""

#~ msgid "Q \\sim \\chi^2_k."
#~ msgstr ""

#~ msgid "The probability density function of the chi-squared distribution is"
#~ msgstr ""

#~ msgid "p(x) = \\frac{(1/2)^{k/2}}{\\Gamma(k/2)} x^{k/2 - 1} e^{-x/2},"
#~ msgstr ""

#~ msgid "where :math:`\\Gamma` is the gamma function,"
#~ msgstr ""

#~ msgid "\\Gamma(x) = \\int_0^{-\\infty} t^{x - 1} e^{-t} dt."
#~ msgstr ""

#~ msgid ""
#~ "NIST \"Engineering Statistics Handbook\" "
#~ "http://www.itl.nist.gov/div898/handbook/eda/section3/eda3666.htm"
#~ msgstr ""

