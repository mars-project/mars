msgid "mars.tensor.random.gamma"
msgstr ""

#~ msgid "Draw samples from a Gamma distribution."
#~ msgstr ""

#~ msgid ""
#~ "Samples are drawn from a Gamma "
#~ "distribution with specified parameters, "
#~ "`shape` (sometimes designated \"k\") and "
#~ "`scale` (sometimes designated \"theta\"), "
#~ "where both parameters are > 0."
#~ msgstr ""

#~ msgid "shape"
#~ msgstr ""

#~ msgid "float or array_like of floats"
#~ msgstr ""

#~ msgid "The shape of the gamma distribution. Should be greater than zero."
#~ msgstr ""

#~ msgid "scale"
#~ msgstr ""

#~ msgid "float or array_like of floats, optional"
#~ msgstr ""

#~ msgid ""
#~ "The scale of the gamma distribution. "
#~ "Should be greater than zero. Default "
#~ "is equal to 1."
#~ msgstr ""

#~ msgid "size"
#~ msgstr ""

#~ msgid "int or tuple of ints, optional"
#~ msgstr ""

#~ msgid ""
#~ "Output shape.  If the given shape "
#~ "is, e.g., ``(m, n, k)``, then ``m"
#~ " * n * k`` samples are drawn."
#~ "  If size is ``None`` (default), a"
#~ " single value is returned if "
#~ "``shape`` and ``scale`` are both "
#~ "scalars. Otherwise, ``np.broadcast(shape, "
#~ "scale).size`` samples are drawn."
#~ msgstr ""

#~ msgid "chunk_size"
#~ msgstr ""

#~ msgid "int or tuple of int or tuple of ints, optional"
#~ msgstr ""

#~ msgid "Desired chunk size on each dimension"
#~ msgstr ""

#~ msgid "gpu"
#~ msgstr ""

#~ msgid "bool, optional"
#~ msgstr ""

#~ msgid "Allocate the tensor on GPU if True, False as default"
#~ msgstr ""

#~ msgid "dtype"
#~ msgstr ""

#~ msgid "data-type, optional"
#~ msgstr ""

#~ msgid "Data-type of the returned tensor."
#~ msgstr ""

#~ msgid "out"
#~ msgstr ""

#~ msgid "Tensor or scalar"
#~ msgstr ""

#~ msgid "Drawn samples from the parameterized gamma distribution."
#~ msgstr ""

#~ msgid "scipy.stats.gamma"
#~ msgstr ""

#~ msgid "probability density function, distribution or"
#~ msgstr ""

#~ msgid "cumulative density function, etc."
#~ msgstr ""

#~ msgid "The probability density for the Gamma distribution is"
#~ msgstr ""

#~ msgid "p(x) = x^{k-1}\\frac{e^{-x/\\theta}}{\\theta^k\\Gamma(k)},"
#~ msgstr ""

#~ msgid ""
#~ "where :math:`k` is the shape and "
#~ ":math:`\\theta` the scale, and :math:`\\Gamma`"
#~ " is the Gamma function."
#~ msgstr ""

#~ msgid ""
#~ "The Gamma distribution is often used "
#~ "to model the times to failure of"
#~ " electronic components, and arises "
#~ "naturally in processes for which the "
#~ "waiting times between Poisson distributed "
#~ "events are relevant."
#~ msgstr ""

#~ msgid ""
#~ "Weisstein, Eric W. \"Gamma Distribution.\" "
#~ "From MathWorld--A Wolfram Web Resource."
#~ " http://mathworld.wolfram.com/GammaDistribution.html"
#~ msgstr ""

