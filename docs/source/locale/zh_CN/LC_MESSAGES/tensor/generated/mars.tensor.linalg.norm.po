msgid "mars.tensor.linalg.norm"
msgstr ""

#~ msgid "Matrix or vector norm."
#~ msgstr ""

#~ msgid ""
#~ "This function is able to return "
#~ "one of eight different matrix norms, "
#~ "or one of an infinite number of"
#~ " vector norms (described below), depending"
#~ " on the value of the ``ord`` "
#~ "parameter."
#~ msgstr ""

#~ msgid "x"
#~ msgstr ""

#~ msgid "array_like"
#~ msgstr ""

#~ msgid "Input tensor.  If `axis` is None, `x` must be 1-D or 2-D."
#~ msgstr ""

#~ msgid "ord"
#~ msgstr ""

#~ msgid "{non-zero int, inf, -inf, 'fro', 'nuc'}, optional"
#~ msgstr ""

#~ msgid ""
#~ "Order of the norm (see table under"
#~ " ``Notes``). inf means mars tensor's "
#~ "`inf` object."
#~ msgstr ""

#~ msgid "axis"
#~ msgstr ""

#~ msgid "{int, 2-tuple of ints, None}, optional"
#~ msgstr ""

#~ msgid ""
#~ "If `axis` is an integer, it "
#~ "specifies the axis of `x` along "
#~ "which to compute the vector norms.  "
#~ "If `axis` is a 2-tuple, it "
#~ "specifies the axes that hold 2-D "
#~ "matrices, and the matrix norms of "
#~ "these matrices are computed.  If `axis`"
#~ " is None then either a vector "
#~ "norm (when `x` is 1-D) or a "
#~ "matrix norm (when `x` is 2-D) is"
#~ " returned."
#~ msgstr ""

#~ msgid "keepdims"
#~ msgstr ""

#~ msgid "bool, optional"
#~ msgstr ""

#~ msgid ""
#~ "If this is set to True, the "
#~ "axes which are normed over are "
#~ "left in the result as dimensions "
#~ "with size one.  With this option "
#~ "the result will broadcast correctly "
#~ "against the original `x`."
#~ msgstr ""

#~ msgid "n"
#~ msgstr ""

#~ msgid "float or Tensor"
#~ msgstr ""

#~ msgid "Norm of the matrix or vector(s)."
#~ msgstr ""

#~ msgid ""
#~ "For values of ``ord <= 0``, the"
#~ " result is, strictly speaking, not a"
#~ " mathematical 'norm', but it may "
#~ "still be useful for various numerical"
#~ " purposes."
#~ msgstr ""

#~ msgid "The following norms can be calculated:"
#~ msgstr ""

#~ msgid "norm for matrices"
#~ msgstr ""

#~ msgid "norm for vectors"
#~ msgstr ""

#~ msgid "None"
#~ msgstr ""

#~ msgid "Frobenius norm"
#~ msgstr ""

#~ msgid "2-norm"
#~ msgstr ""

#~ msgid "'fro'"
#~ msgstr ""

#~ msgid "--"
#~ msgstr ""

#~ msgid "'nuc'"
#~ msgstr ""

#~ msgid "nuclear norm"
#~ msgstr ""

#~ msgid "inf"
#~ msgstr ""

#~ msgid "max(sum(abs(x), axis=1))"
#~ msgstr ""

#~ msgid "max(abs(x))"
#~ msgstr ""

#~ msgid "-inf"
#~ msgstr ""

#~ msgid "min(sum(abs(x), axis=1))"
#~ msgstr ""

#~ msgid "min(abs(x))"
#~ msgstr ""

#~ msgid "0"
#~ msgstr ""

#~ msgid "sum(x != 0)"
#~ msgstr ""

#~ msgid "1"
#~ msgstr ""

#~ msgid "max(sum(abs(x), axis=0))"
#~ msgstr ""

#~ msgid "as below"
#~ msgstr ""

#~ msgid "-1"
#~ msgstr ""

#~ msgid "min(sum(abs(x), axis=0))"
#~ msgstr ""

#~ msgid "2"
#~ msgstr ""

#~ msgid "2-norm (largest sing. value)"
#~ msgstr ""

#~ msgid "-2"
#~ msgstr ""

#~ msgid "smallest singular value"
#~ msgstr ""

#~ msgid "other"
#~ msgstr ""

#~ msgid "sum(abs(x)**ord)**(1./ord)"
#~ msgstr ""

#~ msgid "The Frobenius norm is given by [1]_:"
#~ msgstr ""

#~ msgid ":math:`||A||_F = [\\\\sum_{i,j} abs(a_{i,j})^2]^{1/2}`"
#~ msgstr ""

#~ msgid "The nuclear norm is the sum of the singular values."
#~ msgstr ""

#~ msgid ""
#~ "G. H. Golub and C. F. Van "
#~ "Loan, *Matrix Computations*, Baltimore, MD,"
#~ " Johns Hopkins University Press, 1985, "
#~ "pg. 15"
#~ msgstr ""

