msgid "mars.tensor.copyto"
msgstr ""

#: mars.tensor.copyto:1 of
msgid "Copies values from one array to another, broadcasting as necessary."
msgstr ""

#: mars.tensor.copyto:3 of
msgid ""
"Raises a TypeError if the `casting` rule is violated, and if `where` is "
"provided, it selects which elements to copy."
msgstr ""

#: mars.tensor.copyto:8 of
msgid "dst"
msgstr ""

#: mars.tensor.copyto:7 of
msgid "Tensor"
msgstr ""

#: mars.tensor.copyto:9 of
msgid "The tensor into which values are copied."
msgstr ""

#: mars.tensor.copyto:10 of
msgid "src"
msgstr ""

#: mars.tensor.copyto:9 of
msgid "array_like"
msgstr ""

#: mars.tensor.copyto:11 of
msgid "The tensor from which values are copied."
msgstr ""

#: mars.tensor.copyto:19 of
msgid "casting"
msgstr ""

#: mars.tensor.copyto:18 of
msgid "{'no', 'equiv', 'safe', 'same_kind', 'unsafe'}, optional"
msgstr ""

#: mars.tensor.copyto:13 of
msgid "Controls what kind of data casting may occur when copying."
msgstr ""

#: mars.tensor.copyto:15 of
msgid "'no' means the data types should not be cast at all."
msgstr ""

#: mars.tensor.copyto:16 of
msgid "'equiv' means only byte-order changes are allowed."
msgstr ""

#: mars.tensor.copyto:17 of
msgid "'safe' means only casts which can preserve values are allowed."
msgstr ""

#: mars.tensor.copyto:18 of
msgid ""
"'same_kind' means only safe casts or casts within a kind, like float64 to"
" float32, are allowed."
msgstr ""

#: mars.tensor.copyto:20 of
msgid "'unsafe' means any data conversions may be done."
msgstr ""

#: mars.tensor.copyto:23 of
msgid "where"
msgstr ""

#: mars.tensor.copyto:22 of
msgid "array_like of bool, optional"
msgstr ""

#: mars.tensor.copyto:22 of
msgid ""
"A boolean tensor which is broadcasted to match the dimensions of `dst`, "
"and selects elements to copy from `src` to `dst` wherever it contains the"
" value True."
msgstr ""

