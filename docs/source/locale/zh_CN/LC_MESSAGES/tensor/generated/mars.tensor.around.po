msgstr ""

#: mars.tensor.around:1 of
msgid "Evenly round to the given number of decimals."
msgstr ""

#: mars.tensor.around:5 of
msgid "a"
msgstr ""

#: mars.tensor.around:4 of
msgid "array_like"
msgstr ""

#: mars.tensor.around:6 of
